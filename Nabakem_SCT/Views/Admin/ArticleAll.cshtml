@{
    ViewBag.Title = "ArticleAll";
    Layout = "~/Views/_Layouts/AdminLayout.cshtml";
}

<h2>ArticleAll</h2>
<table id="article-all-list" class="tbl_bbs_list">
    <tr>
        <th>구분</th>
        <th>카테고리</th>
        <th>제목</th>
        <th>작성자</th>
        <th>작성일</th>
        <th>조회수</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Bbs</td>
            <td>@item.Category</td>
            <td>@item.Subject</td>
            <td>@item.Author</td>
            <td>@item.InsertDate</td>
            <td>@item.Hit</td>
        </tr>
    }
</table>